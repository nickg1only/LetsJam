{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "/",
  "type": "object",
  "properties": {
    "name": {
      "id": "name",
      "type": "string"
    },
    "info": {
      "id": "info",
      "type": "string"
    },
    "location": {
      "id": "location",
      "type": "string"
    },
    "members": {
      "id": "members",
      "type": "array",
      "items": {
          "type": "integer"
      },
      "minItems": 1,
      "uniqueItems": true
    },
    "wanted": {
      "id": "wanted",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
           "instrument": {"type": "string"},
           "info": {"type": "string"}
        },
        "required": ["instument", "info"]
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "name",
    "info",
    "location"
  ]
}
